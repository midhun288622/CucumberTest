node() {
   // adds job parameters within jenkinsfile
   properties([
     parameters([
       stringParam(
         defaultValue: dasda,
         description: 'environment rfgsgs',
         name: 'environment'
       ),
       stringParam(
         defaultValue: '10',
         description: 'threadCount srfgsd',
         name: 'threadCount'
       ),
     ])
   ])

   // test the false value
   print 'DEBUG: parameter isFoo = ' + params.environment
   print "DEBUG: parameter isFoo = ${params.environment}"
 
}
