node {
   properties([
  parameters([
    string(name: 'DEPLOY_ENV', defaultValue: 'TESTING', description: 'The target environment', )
   ])
])

     echo "Will deploy to ${params.DEPLOY_ENV}"
   }
   
   
   
pipeline {
    agent any
    stages {
        
        
    
         
          stage ("Change flght status") 
        {
            steps {
             script {
             
                   echo "Will deploy to ${params.DEPLOY_ENV}"
     
                  }
       
               }
         }
         
       
   }
}
   
   